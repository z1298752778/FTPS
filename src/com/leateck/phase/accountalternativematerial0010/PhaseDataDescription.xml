<RtPhaseDataAttributes>    
    <RtPhaseDataAttribute>
        <Name>materialID</Name>
        <Description>Material input ID</Description>
        <DataType>
            <String length="64"/>
        </DataType>
    </RtPhaseDataAttribute>   
    <RtPhaseDataAttribute>
        <Name>mfcPos</Name>
        <Description>Material MFC position</Description>
        <DataType>
            <String length="64"/>
        </DataType>
    </RtPhaseDataAttribute>
    <RtPhaseDataAttribute>
        <Name>isHeader</Name>
        <Description>Is this a header line containing summarized data about a material</Description>
        <DataType>
            <Bool/>
        </DataType>
    </RtPhaseDataAttribute>   
    <RtPhaseDataAttribute>
        <Name>materialDescription</Name>
        <Description>Material input Description</Description>
        <DataType>
            <String length="50"/>
        </DataType>
    </RtPhaseDataAttribute>
    <RtPhaseDataAttribute>
        <Name>batchID</Name>
        <Description>Batch ID of the accounted sublot</Description>
        <DataType>
            <String length="128"/>
        </DataType>
    </RtPhaseDataAttribute>
    <RtPhaseDataAttribute>
        <Name>sublotID</Name>
        <Description>ID of the accounted sublot</Description>
        <DataType>
            <String length="128"/>
        </DataType>
    </RtPhaseDataAttribute>
    <RtPhaseDataAttribute>
        <Name>identifiedQty</Name>
        <Description>Identified quantity for this material (sublot)</Description>
        <DataType>
            <MeasuredValue/>
        </DataType>
    </RtPhaseDataAttribute>
    <RtPhaseDataAttribute>
        <Name>consumedQty</Name>
        <Description>Consumed quantity for this material (sublot)</Description>
        <DataType>
            <MeasuredValue/>
        </DataType>
    </RtPhaseDataAttribute>
    <RtPhaseDataAttribute>
        <Name>wastedQty</Name>
        <Description>Wasted quantity for this material (sublot)</Description>
        <DataType>
            <MeasuredValue/>
        </DataType>
    </RtPhaseDataAttribute>
    <RtPhaseDataAttribute>
        <Name>sampledQty</Name>
        <Description>Sampled quantity for this material (sublot)</Description>
        <DataType>
            <MeasuredValue/>
        </DataType>
    </RtPhaseDataAttribute>
    <RtPhaseDataAttribute>
        <Name>returnedQty</Name>
        <Description>Returned quantity for this material (sublot)</Description>
        <DataType>
            <MeasuredValue/>
        </DataType>
    </RtPhaseDataAttribute>
     <RtPhaseDataAttribute>
        <Name>plannedQtyWithLimits</Name>
        <Description>Planned quantity with limits for this material (relevant only in the header row)</Description>
        <DataType>
            <String length="64"/>
        </DataType>
      </RtPhaseDataAttribute>
      <RtPhaseDataAttribute>
        <Name>accountingStatus</Name>
        <Description>Accounting status (out of range and accounting complete) for this material (relevant only in the header row)</Description>
        <DataType>
            <String length="64"/>
        </DataType>
     </RtPhaseDataAttribute>
    <RtPhaseDataAttribute>
        <Name>result</Name>
        <Description>CONTINUE: The value is CONTINUE if phase was completed with the Continue option button.
            DONE: The value is DONE if phase was completed with the Done option button.
        </Description>
        <DataType>
            <String length="20" />
        </DataType>
    </RtPhaseDataAttribute>
    <RtPhaseDataAttribute>
        <Name>isModified</Name>
        <Description>Whether any sublot of this material position was modified in this phase instance (relevant only in the header row)</Description>
        <DataType>
            <Bool/>
        </DataType>
    </RtPhaseDataAttribute>  
</RtPhaseDataAttributes>

